def caught_speeding(speed, is_birthday):
    if (speed <= 60 and is_birthday == False) or (speed <= 60+5 and is_birthday == True):
        print("No Ticket")
    elif(speed > 60 and speed <= 80 and is_birthday == False) or (speed > 60+5 and speed <= 80+5 and is_birthday == True):
        print("Small Ticket")
    elif(speed >= 81 and is_birthday == False) or (speed >= 81+5 and is_birthday == True):
        print("Big Ticket")
